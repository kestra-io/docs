---
import {filterPluginsWithoutDeprecated, type Plugin } from "@kestra-io/ui-libs";
import SimilarPluginsVue from "~/components/plugins/SimilarPlugins.vue";

interface Props{
    allPlugins: Plugin[];
    pluginsData: Record<string, PluginInformation>;
}

const props = Astro.props;
const { allPlugins } = props;

---

<SimilarPluginsVue
    similarPlugins={filterPluginsWithoutDeprecated(allPlugins ?? [])}
    pluginsData={props.pluginsData}
    client:visible
/>