{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "assets/*": ["./src/assets/*"],
      "~/composables/*": [
        "./composables/*",
        "./src/composables/*"
      ],
      "~/utils/*": [
        "./utils/*",
        "./src/utils/*"
      ],
    }
  }
}