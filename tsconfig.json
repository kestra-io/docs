{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "preserve",
    "paths": {
      "~/assets/*": ["./src/assets/*"],
      "~/composables/*": ["./src/composables/*"],
      "~/utils/*": ["./src/utils/*"],
      "~/components/*": ["./src/components/*"]
    }
  }
}